{
  "vhosts": {
    "/": {
      "connection": {
        "url": "amqp://guest:guest@localhost:5672/"
      },
      "exchanges": ["social_aggregation_ddd"],
      "queues": ["platform_access"],
      "bindings": ["social_aggregation_ddd[user_registered] -> platform_access"],
      "publications": {
        "platform_access.user_registered": {
          "exchange": "social_aggregation_ddd",
          "routingKey": "user_registered"
        }
      },
      "subscriptions": {
        "platform_access.user_registered": {
          "queue": "platform_access",
          "prefetch": 3
        }
      }
    }
  }
}
